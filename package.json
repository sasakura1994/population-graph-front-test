    "@storybook/testing-vue": "^0.0.4",
    "@storybook/vue3": "^7.5.2",
    "@storybook/vue3-vite": "^7.5.2",
    "@swc/core": "^1.3.95",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.4",
